{
  "name": "hud-manager",
  "version": "1.9.2",
  "homepage": "./",
  "description": "Lexogrine HUD Manager",
  "author": "Lexogrine",
  "private": true,
  "dependencies": {
    "@node-steam/vdf": "^2.1.0",
    "big-integer": "^1.6.48",
    "bootstrap": "^4.4.1",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "csgogsi": "^1.2.6",
    "csgogsi-generator": "^1.0.1",
    "csgogsi-socket": "^1.2.8",
    "decompress-zip": "^0.2.2",
    "electron-updater": "^4.3.5",
    "express": "^4.17.1",
    "get-port": "^5.1.1",
    "internal-ip": "^6.2.0",
    "ip": "^1.1.5",
    "json5": "^2.1.3",
    "jsonwebtoken": "^8.5.1",
    "nedb": "^1.8.0",
    "node-abi": "^2.19.3",
    "node-fetch": "^2.6.1",
    "node-media-server": "^2.2.4",
    "portscanner": "^2.2.0",
    "public-ip": "^4.0.3",
    "pug": "^2.0.4",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-color": "^2.19.3",
    "react-dom": "^16.13.1",
    "react-scripts": "^4.0.0",
    "react-select-country-list": "^2.1.2",
    "reactstrap": "^8.4.1",
    "registry-js": "^1.13.0",
    "request": "^2.88.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "steam-game-path": "^1.1.1",
    "typescript": "^3.8.3",
    "uuid": "^3.4.0",
    "ws": "^7.2.5",
    "yargs": "^15.3.1"
  },
  "build": {
    "appId": "com.lexogrine.hudmanager",
    "productName": "Lexogrine HUD Manager",
    "win": {
      "icon": "./dist/build/favicon.ico",
      "target": "NSIS"
    },
    "linux": {
      "target": "tar.xz"
    },
    "directories": {
      "output": "app",
      "buildResources": "assets"
    },
    "nsis": {
      "license": "license.txt"
    },
    "files": [
      "boltobserv/**/*",
      "dist/**/*",
      "package.json",
      {
        "from": "dist",
        "to": "build",
        "filter": "**/*"
      },
      "!**/*.asar",
      "!**/app/**/*",
      "!**/app/*",
      "!**/win-unpacked/*",
      "!**/.git/**/*",
      "!**/.git/*"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "lexogrine",
        "repo": "hud-manager"
      }
    ]
  },
  "main": "dist/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run transpile && react-scripts build && rm -rf dist/build &&  mv build dist",
    "test": "react-scripts test",
    "transpile": "rm -rf dist/boltobserv && cp -r boltobserv dist && tsc electron.ts RMTPServer.ts --outDir dist --esModuleInterop true",
    "devElectron": "cross-env DEV=true electron ./",
    "electron": "npm run transpile && npm run devElectron",
    "dev": "concurrently \"cross-env BROWSER=none npm start\" \"npm run electron\"",
    "eject": "react-scripts eject",
    "dist": "electron-builder --publish never",
    "publish": "electron-builder --publish always",
    "compile": "npm run build && npm run dist",
    "compileAndPublish": "npm run build && npm run publish",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier-format": "prettier --config .prettierrc --write **/*.{ts,tsx}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/ip": "^1.1.0",
    "@types/jest": "24.0.18",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/nedb": "^1.8.9",
    "@types/node": "^12.12.37",
    "@types/react": "^17.0.0",
    "@types/react-color": "^3.0.4",
    "@types/react-dom": "^17.0.0",
    "@types/reactstrap": "^8.4.2",
    "@types/request": "^2.48.4",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/uuid": "^3.4.9",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "concurrently": "^5.2.0",
    "electron": "^11.1.1",
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.10.1",
    "eslint": "^7.11.0",
    "eslint-plugin-react": "^7.20.5",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "wait-on": "^3.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-format && npm run lint"
    }
  }
}
